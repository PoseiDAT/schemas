{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://poseidat.org/schema/core/ais-DGNSS-data.json",
  "title": "AIS DGNSS data",
  "description": "AIS DGNSS data send with an AIS message",
  "properties": {
    "z_count": {
      "type":"number",
      "description": "Time value in 0.6 s (0-3 599.4)"
    },
    "sequence_number": {
      "description": "Message sequence number (cyclic 0-7)",
       "type": "number"
    },
    "number_of_words": {
      "type": "number",
      "description": "The number of words in the GNSS data message"
    },
    "health": {
      "type": "number",
      "description" : "The station health"
    },
    "DGNSS_data": {
      "type": "string",
      "description": "DGNSS message data words excluding parity"
    }
  },
  "required": [
  ],
  "type": "object",
  "additionalProperties": false
}
